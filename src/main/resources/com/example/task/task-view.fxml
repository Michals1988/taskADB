<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="764.0" prefWidth="895.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.task.TaskController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="clock" text="HH:mm:ss">
        <font>
            <Font size="43.0" />
        </font>
    </Label>
    <Pane prefHeight="706.0" prefWidth="844.0">
        <children>
            <ListView fx:id="channelList" layoutX="13.0" layoutY="60.0" prefHeight="475.0" prefWidth="214.0" />
            <Button fx:id="choosenChannel" layoutX="56.0" layoutY="547.0" mnemonicParsing="false" onAction="#onChoosenChannel" text="Wybierz program" />
            <Button fx:id="JsonPath" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onJsonPath" prefHeight="30.0" prefWidth="115.0" text="Wybierz" />
            <Label fx:id="jsonPathLabel" layoutX="142.0" layoutY="21.0" text="Wybierz folder w którym znajdują się pliki .Json">
                <font>
                    <Font size="16.0" />
                </font>
            </Label>
            <Label fx:id="channelName" layoutX="371.0" layoutY="116.0" prefHeight="30.0" prefWidth="438.0">
                <font>
                    <Font size="22.0" />
                </font>
            </Label>
            <ImageView fx:id="channelLogo" fitHeight="50.0" fitWidth="100.0" layoutX="235.0" layoutY="106.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="currentEvent" layoutX="235.0" layoutY="228.0" prefHeight="50.0" prefWidth="621.0">
                <font>
                    <Font name="Khmer OS System" size="19.0" />
                </font>
            </Label>
            <Label layoutX="236.0" layoutY="280.0" text="Godzina rozpoczęcia programu:" />
            <Label layoutX="235.0" layoutY="306.0" text="Godzina zakończenia programu:" />
            <Label fx:id="startTimeLabel" layoutX="486.0" layoutY="280.0" prefHeight="18.0" prefWidth="171.0" text="HH:MM" />
            <Label fx:id="endTimeLabel" layoutX="486.0" layoutY="306.0" prefHeight="18.0" prefWidth="171.0" text="HH:MM" />
            <Label fx:id="currentEvent1" layoutX="235.0" layoutY="200.0" prefHeight="38.0" prefWidth="621.0" text="Tytuł programu:">
                <font>
                    <Font name="Khmer OS System" size="14.0" />
                </font>
            </Label>
            <Label fx:id="currentEvent11" layoutX="371.0" layoutY="68.0" prefHeight="18.0" prefWidth="249.0" text="Kanał telewizyjny:">
                <font>
                    <Font name="Khmer OS System" size="15.0" />
                </font>
            </Label>
        </children>
    </Pane>
</VBox>
